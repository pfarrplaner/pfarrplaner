{
    "name": "potofcoffee/pfarrplaner",
    "type": "project",
    "description": "Service and vacation planning for a district of Lutheran churches",
    "license": "GPL-3.0-or-later",
    "version": "2022.143.2",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/potofcoffee/laravel-database-encryption.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/potofcoffee/PHPWord"
        },
        {
            "type": "vcs",
            "url": "https://github.com/potofcoffee/PHPPresentation"
        }
    ],
    "require": {
        "php": "^8.0",
        "almasaeed2010/adminlte": "~3.0",
        "austinheap/laravel-database-encryption": "dev-laravel8",
        "barryvdh/laravel-ide-helper": "^2.6",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.0",
        "inertiajs/inertia-laravel": "^0.3.4",
        "laravel-lang/lang": "~8.0",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.9",
        "laravel/tinker": "^2.6.1",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-sftp": "~1.0",
        "mpdf/qrcode": "^1.0",
        "nesbot/carbon": "^2.44",
        "niklasravnsborg/laravel-pdf": "^4.0",
        "php-ews/php-ews": "^1.0",
        "phpoffice/phppresentation": "dev-fix-video",
        "phpoffice/phpspreadsheet": "^1.8",
        "phpoffice/phpword": "dev-develop",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "revolution/laravel-amazon-product-api": "^4.5",
        "shetabit/visitor": "^2.0",
        "simplesoftwareio/simple-qrcode": "^4.1",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-permission": "^3.13",
        "staudenmeir/belongs-to-through": "^2.3",
        "thybag/php-sharepoint-lists-api": "dev-master",
        "tightenco/ziggy": "^1.0",
        "ukfast/laravel-health-check": "^1.0",
        "venturecraft/revisionable": "1.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.9.2",
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    }
}
