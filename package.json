{
    "version": "1.64.0",
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-prod": "npm run production -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prepare": "husky install",
        "release": "standard-version",
        "release:minor": "standard-version --release-as minor",
        "release:patch": "standard-version --release-as patch",
        "release:major": "standard-version --release-as major",
        "licenses:update": "license-checker --json > .npm-licenses && composer licenses --format=json > .composer-licenses && php artisan licenses:markdown && git add CREDITS.md && git commit -m \"chore: Updating license information\""
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "axios": "^0.21",
        "bootstrap": "^4.0.0",
        "cross-env": "^5.1",
        "github-contributors": "^0.4.1",
        "husky": "^6.0.0",
        "jquery": "3.5.0",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "moment-locales-webpack-plugin": "^1.2.0",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.22.10",
        "sass-loader": "^7.1.0",
        "standard-version": "^9.3.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@inertiajs/inertia": "^0.7.0",
        "@inertiajs/inertia-vue": "^0.4.3",
        "@inertiajs/progress": "^0.2.4",
        "@isneezy/vue-selectize": "^0.6.1",
        "admin-lte": "^3.0.5",
        "bootstrap-datepicker": "^1.9.0",
        "jquery-ui": "^1.12.1",
        "moment-timezone": "^0.5.33",
        "selectize": "^0.12.6",
        "tempusdominus-bootstrap-4": "^5.1.2",
        "vue-bootstrap-datetimepicker": "^5.0.1",
        "vue-markdown": "^2.2.4",
        "vue-nl2br": "^0.1.2",
        "vue-quill-editor": "^3.0.6",
        "vue2-selectize": "^1.1.7",
        "vuedraggable": "^2.24.3"
    }
}
